.login-content{
  position: fixed;
  border: 2px solid rgba(107, 101, 101,0.8);
  border-radius: 10%;
  width: 690rpx;
  margin: 40% 30rpx;
  padding: 40rpx 0;
}
.login-text {
  color: white;
  margin: 50rpx 10px;
  height: 30rpx;
  padding: 10px 15px;
}

.show-item{
  border: 2px solid rgba(107, 101, 101,0.5);
}

.select-item{
  border: 2px solid rgba(37, 211, 52, 0.7);
}

.login-btn{
  margin: 15px;
  border: 2px solid rgba(107, 101, 101,0.3);
  background-color: rgb(30, 200, 58)
}

.title{
  color: white;
  text-align: center;
  position: fixed;
  bottom: 20rpx;
  left: 0;
  right: 0;
}